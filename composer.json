{
    "name": "beseda/online-library-api",
    "description": "Online Library API",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Beseda\\OnlineLibraryApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Oleksiy Beseda",
            "email": "beseda.odessa@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/skeleton": "^7.4",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6",
        "lexik/jwt-authentication-bundle": "^3.2",
        "symfony/flex": "^2.10"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.65"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
